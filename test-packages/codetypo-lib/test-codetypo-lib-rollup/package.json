{
  "name": "test-codetypo-lib-rollup",
  "version": "8.17.2",
  "description": "Pure testing package for codetypo-lib.",
  "bin": {
    "mjs": "bin.mjs",
    "csj": "bin.cjs"
  },
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf dist temp coverage \"*.tsbuildInfo\"",
    "build": "pnpm build:plugin && pnpm build:rollup",
    "build:plugin": "tsc -p plugin/tsconfig.json",
    "build:rollup": "rollup -c rollup.config.mjs",
    "clean-build": "pnpm run clean && pnpm run build",
    "compile": "tsc -p .",
    "test": "pnpm test:esm && pnpm test:cjs",
    "test:esm": "node bin.mjs",
    "test:cjs": "node bin.cjs"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@codetypo/codetypo-bundled-dicts": "workspace:*",
    "import-fresh": "^3.3.0"
  },
  "devDependencies": {
    "codetypo-lib": "workspace:*",
    "magic-string": "^0.30.17"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "engines": {
    "node": ">=18"
  },
  "keywords": []
}
