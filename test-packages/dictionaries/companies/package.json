{
  "name": "test-dict-companies",
  "version": "8.17.2",
  "private": true,
  "description": "Company names dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli compile --no-compress --split \"src/companies.txt\" -o ./dict",
    "test": "codetypo -c ./codetypo-ext.json --local=* --languageId=* src/companies.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khulnasoft/codetypo.git"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "companies",
    "Company names",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/companies#readme",
  "devDependencies": {
    "@codetypo/codetypo-tools": "workspace:*",
    "codetypo": "workspace:*"
  },
  "files": [
    "companies.txt.gz",
    "codetypo-ext.json"
  ]
}
