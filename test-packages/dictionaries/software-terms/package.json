{
  "name": "test-dict-software-terms",
  "version": "8.17.2",
  "private": true,
  "description": "Software terms dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "codetypo -c ./codetypo-ext.json \"--local=*\" \"--languageId=*\" src/*.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khulnasoft/codetypo.git"
  },
  "keywords": [
    "codetypo",
    "software-terms",
    "software terms",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/software-terms#readme",
  "devDependencies": {
    "@codetypo/codetypo-tools": "workspace:*",
    "codetypo": "workspace:*"
  },
  "files": [
    "dict/*",
    "codetypo-ext.json"
  ]
}
