{
  "name": "@codetypo/dict-django",
  "version": "4.1.4",
  "description": "Django dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli compile --keep-raw-case \"src/*.txt\" -M django --no-compress -o ./dict",
    "test": "shx head -n 100 src/django.txt | codetypo -c ./codetypo-ext.json --locale=* --languageId=python stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/django"
  },
  "keywords": [
    "codetypo",
    "Django",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "contributors": [
    "canarduck",
    "Jason Dent <jason@khulnasoft.nl>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/django#readme",
  "files": [
    "dict/django.txt",
    "codetypo-ext.json"
  ]
}
