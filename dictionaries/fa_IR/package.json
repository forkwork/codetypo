{
  "name": "@codetypo/dict-fa-ir",
  "version": "4.0.3",
  "description": "Persian dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "build:conditional": "codetypo-tools-cli build --conditional && pnpm gz",
    "gz": "codetypo-tools-cli gzip \"*.trie\"",
    "test": "codetypo samples --no-progress",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm gz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/fa_IR"
  },
  "keywords": [
    "codetypo",
    "fa_IR",
    "Persian",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/fa_IR#readme",
  "files": [
    "fa-IR.trie.gz",
    "codetypo-ext.json"
  ]
}
