{
  "name": "@codetypo/dict-flutter",
  "version": "1.1.0",
  "description": "Flutter dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "npm run test-dict && npm run test-samples",
    "test-samples": "codetypo \"samples/**/*.dart\"",
    "test-dict": "shx head -n 1000 \"src/flutter.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=dart\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/flutter"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "dart",
    "flutter",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/flutter#readme",
  "files": [
    "flutter.txt.gz",
    "codetypo-ext.json"
  ]
}
