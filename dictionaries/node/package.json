{
  "name": "@codetypo/dict-node",
  "version": "5.0.6",
  "description": "Node.js dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build-node": "node src/extract-api.mjs src/api/*.md",
    "build": "codetypo-tools-cli build",
    "test": "pnpm test-dict",
    "test-dict": "shx cat \"src/node.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=javascript\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/node"
  },
  "keywords": [
    "codetypo",
    "node",
    "Node.js",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/node#readme",
  "files": [
    "dict/*.txt",
    "codetypo-ext.json"
  ]
}
