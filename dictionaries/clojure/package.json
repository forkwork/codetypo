{
  "name": "@codetypo/dict-clojure",
  "version": "2.0.6",
  "description": "Clojure dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "shx head -n 1000 \"src/clojure.txt\" | codetypo -c ./codetypo-ext.json \"--locale=en\" \"--languageId=clojure\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/clojure"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "clojure",
    "clojure",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/clojure#readme",
  "files": [
    "clojure.txt.gz",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
