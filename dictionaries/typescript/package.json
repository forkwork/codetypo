{
  "name": "@codetypo/dict-typescript",
  "version": "3.2.0",
  "description": "TypeScript and JavaScript dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "shx head -n 100 src/typescript.txt | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=typescript\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasofto-dicts",
    "directory": "dictionaries/typescript"
  },
  "keywords": [
    "codetypo",
    "typescript",
    "TypeScript and JavaScript",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasofto-dicts/issues"
  },
  "homepage": "https://github.com/khulnasofto-dicts/blob/main/dictionaries/typescript#readme",
  "files": [
    "dict/typescript.txt",
    "codetypo-ext.json"
  ]
}
