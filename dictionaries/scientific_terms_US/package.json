{
  "name": "@codetypo/dict-scientific-terms-us",
  "version": "3.0.6",
  "description": "Scientific Terms US dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build && pnpm gz",
    "conditional-build": "codetypo-tools-cli build --conditional && pnpm gz",
    "test": "shx head -n 1000 \"src/custom_scientific_US.dic.txt\" | codetypo -c ./codetypo-ext.json --locale=en --languageId=* stdin",
    "gz": "codetypo-tools-cli gzip \"*.trie\"",
    "prepare:dictionary": "pnpm gz",
    "prepublishOnly": "echo pre-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasofto-dicts",
    "directory": "dictionaries/scientific_terms_US"
  },
  "keywords": [
    "codetypo",
    "scientific_terms_US",
    "Scientific Terms US",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasofto-dicts/issues"
  },
  "homepage": "https://github.com/khulnasofto-dicts/blob/main/dictionaries/scientific_terms_US#readme",
  "files": [
    "custom_scientific_US.trie.gz",
    "codetypo-ext.json"
  ]
}
