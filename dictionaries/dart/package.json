{
  "name": "@codetypo/dict-dart",
  "version": "2.3.0",
  "description": "Dart dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "npm run test-dict && npm run test-samples",
    "test-samples": "codetypo \"samples/**/*.dart\"",
    "test-dict": "codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=dart\" samples/dart-common.txt",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/dart"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "dart",
    "dart",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/dart#readme",
  "files": [
    "dart.txt.gz",
    "codetypo-ext.json"
  ]
}
