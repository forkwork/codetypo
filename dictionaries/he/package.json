{
  "name": "@codetypo/dict-he",
  "version": "4.0.3",
  "description": "Hebrew dictionary for codetypo",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build && pnpm gz",
    "build:conditional": "codetypo-tools-cli build --conditional && pnpm gz",
    "test": "codetypo samples",
    "gz": "codetypo-tools-cli gzip \"*.trie\"",
    "prepare:dictionary": "pnpm gz",
    "prepublishOnly": "echo pre-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/he"
  },
  "keywords": [
    "codetypo",
    "he",
    "Hebrew",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "contributors": [
    "Noam Ben Shabat"
  ],
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/he#readme",
  "files": [
    "he.trie.gz",
    "codetypo-ext.json",
    "!samples"
  ]
}
