{
  "name": "@codetypo/dict-vim",
  "version": "1.0.8",
  "description": "Vim dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 codetypo-tools-cli build",
    "test": "head -n 1000 \"src/vim.txt\" | codetypo \"--locale=*\" \"--languageId=*\" stdin",
    "prepublishOnly": "echo OK",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasofto-dicts",
    "directory": "dictionaries/vim"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "vim",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft
  "contributors": [
    "Kevin Traver"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasofto-dicts/issues"
  },
  "homepage": "https://github.com/khulnasofto-dicts/blob/main/dictionaries/vim#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "dict/vim.txt",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
