{
  "name": "@codetypo/dict-filetypes",
  "version": "3.0.10",
  "description": "Filetypes dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "shx cat \"src/filetypes.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=*\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/filetypes"
  },
  "keywords": [
    "codetypo",
    "filetypes",
    "filetypes",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/filetypes#readme",
  "files": [
    "filetypes.txt.gz",
    "codetypo-ext.json"
  ]
}
