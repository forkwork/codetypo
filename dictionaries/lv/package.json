{
  "name": "@codetypo/dict-lv",
  "version": "1.1.4",
  "description": "Latvian dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "build:conditional": "pnpm sync && codetypo-tools-cli build --conditional && pnpm gz",
    "sync": "pnpm cpy \"node_modules/dictionary-lv/{index.{aff,dic},license,readme.md}\" \"src/hunspell/dictionary-lv/\" --flat",
    "gz": "codetypo-tools-cli gzip \"dict/*.trie\"",
    "test": "codetypo samples --no-progress",
    "prepare:dictionary": "pnpm gz",
    "prepublishOnly": "echo pre-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/lv"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "lv",
    "Latvian",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "LGPL-3.0+",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/lv#readme",
  "files": [
    "dict/lv.trie.gz",
    "codetypo-ext.json",
    "src/hunspell/dictionary-lv/license",
    "src/hunspell/dictionary-lv/readme.md",
    "*.js",
    "*.d.ts"
  ],
  "devDependencies": {
    "dictionary-lv": "^2.0.0"
  }
}
