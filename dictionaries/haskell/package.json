{
  "name": "@codetypo/dict-haskell",
  "version": "4.0.5",
  "description": "Haskell dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli compile --keep-raw-case src/haskell.txt --no-compress -o ./dict",
    "test": "shx head -n 100 src/haskell.txt | codetypo -c ./codetypo-ext.json --locale=* --languageId=haskell stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/haskell"
  },
  "keywords": [
    "codetypo",
    "haskell",
    "Haskell",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/haskell#readme",
  "files": [
    "./dict/haskell.txt",
    "codetypo-ext.json"
  ]
}
