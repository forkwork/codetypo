{
  "name": "@codetypo/dict-en-shared",
  "version": "1.0.20",
  "description": "English words shared between the various English dictionaries. -- Private until verified",
  "private": true,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "shx head -n 1000 \"src/shared-additional-words.txt\" | codetypo -c ./codetypo-ext.json \"--locale=en\" \"--languageId=*\" stdin",
    "prepublishOnly": "echo OK",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/en_shared"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "en_shared",
    "en_shared",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/en_shared#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "dict/en-shared.txt",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
