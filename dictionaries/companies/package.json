{
  "name": "@codetypo/dict-companies",
  "version": "3.1.13",
  "description": "Company names dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli compile --no-compress --split \"src/companies.txt\" -o ./dict",
    "test": "shx cat \"src/companies.txt\" | codetypo -c ./codetypo-ext.json --locale=* --languageId=* stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/companies"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "companies",
    "Company names",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/companies#readme",
  "files": [
    "dict/*",
    "codetypo-ext.json"
  ]
}
