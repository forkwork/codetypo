{
  "name": "@codetypo/dict-fonts",
  "version": "4.0.4",
  "description": "Font names dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli compile --keep-raw-case --use-legacy-splitter src/fonts.txt --no-compress -o ./dict",
    "test": "shx head -n 100 src/fonts.txt | codetypo -c ./codetypo.json \"--locale=*\" \"--languageId=*\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/fonts"
  },
  "keywords": [
    "codetypo",
    "fonts",
    "font names",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/fonts#readme",
  "files": [
    "dict/fonts.txt",
    "codetypo-ext.json"
  ]
}
