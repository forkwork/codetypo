{
  "name": "@codetypo/dict-docker",
  "version": "1.1.12",
  "description": "Docker dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "shx head -n 1000 \"src/docker-words.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=dockerfile\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/docker"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "docker",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/docker#readme",
  "files": [
    "docker-words.txt.gz",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
