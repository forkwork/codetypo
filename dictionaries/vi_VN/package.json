{
  "name": "@codetypo/dict-vi-vn",
  "version": "3.0.4",
  "description": "Vietnamese dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build && pnpm gz",
    "build:conditional": "codetypo-tools-cli build --conditional && pnpm gz",
    "test": "codetypo samples",
    "gz": "codetypo-tools-cli gzip *.trie",
    "prepare:dictionary": "pnpm run gz",
    "prepublishOnly": "echo pre-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/vi_VN"
  },
  "keywords": [
    "codetypo",
    "vi_VN",
    "Vietnamese",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/vi_VN#readme",
  "files": [
    "vi.trie.gz",
    "codetypo-ext.json"
  ]
}
