{
  "name": "@codetypo/dict-markdown",
  "version": "2.0.9",
  "description": "Markdown dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "pnpm run test-check",
    "test-check": "ava",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/markdown"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "markdown",
    "Markdown",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft",  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/markdown#readme",
  "devDependencies": {},
  "dependencies": {},
  "peerDependencies": {
    "@codetypo/dict-css": "workspace:^",
    "@codetypo/dict-html": "workspace:^",
    "@codetypo/dict-html-symbol-entities": "workspace:^",
    "@codetypo/dict-typescript": "workspace:^"
  },
  "files": [
    "markdown.txt.gz",
    "codetypo-ext.json",
    "codetypo-ext.yaml",
    "*.js",
    "*.d.ts"
  ]
}
