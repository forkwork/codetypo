{
  "name": "@codetypo/dict-fi-fi",
  "version": "1.1.1",
  "description": "Finnish dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "conditional-build": "pnpm run build --conditional && pnpm gz",
    "test": "pnpm run test:words && pnpm run test:samples",
    "test:samples": "codetypo samples",
    "test:words": "pipe-cmds \"hunspell-reader words -n 1000 -m 0 \\\"src/hunspell/fi_FI.dic\\\"\" \"codetypo -c ./codetypo-ext.json \\\"--locale=fi\\\" \\\"--languageId=*\\\" stdin\"",
    "gz": "codetypo-tools-cli gzip \"dict/*.trie\"",
    "prepare:dictionary": "pnpm gz",
    "prepublishOnly": "echo OK"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/fi_FI"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "fi_FI",
    "Finnish",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "contributors": [
    "Kimi Heinonen <kimi.heinonen+codetypo@gmail.com>"
  ],
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/fi_FI#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "dict/fi-fi.trie.gz",
    "src/hunspell/LICENSE",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
