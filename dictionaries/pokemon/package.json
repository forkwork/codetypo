{
  "name": "@codetypo/dict-pokemon",
  "version": "1.0.1",
  "description": "Pokémon dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "head -n 1000 \"src/pokemon.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=*\" stdin",
    "prepublishOnly": "echo OK",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/pokemon"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "pokemon",
    "Pokémon",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft",  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/pokemon#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "dict/pokemon.txt",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
