{
  "name": "@codetypo/dict-pl_pl",
  "version": "3.0.4",
  "description": "Polish dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build && pnpm gz",
    "conditional-build": "codetypo-tools-cli build --conditional && pnpm gz",
    "test": "codetypo samples",
    "gz": "codetypo-tools-cli gzip \"*.trie\"",
    "prepare:dictionary": "pnpm gz",
    "prepublishOnly": "echo pre-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/pl_PL"
  },
  "keywords": [
    "codetypo",
    "pl_PL",
    "Polish",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft.nl>",
  "license": "LGPL-3.0+",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/pl_PL#readme",
  "files": [
    "pl_pl.trie.gz",
    "src/hunspell/license",
    "codetypo-ext.json"
  ]
}
