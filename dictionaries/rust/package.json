{
  "name": "@codetypo/dict-rust",
  "version": "4.0.11",
  "description": "Rust dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli compile --keep-raw-case src/rust.txt --no-compress -o ./dict",
    "test": "shx head -n 1000 src/rust.txt | codetypo -c ./codetypo-ext.json --locale=* --languageId=rust stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/rust"
  },
  "keywords": [
    "codetypo",
    "rust",
    "Rust",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft",
  "contributors": [
    "Alexander Andreev"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/rust#readme",
  "files": [
    "dict/rust.txt",
    "codetypo-ext.json"
  ]
}
