{
  "name": "@codetypo/dict-sr-cyrl",
  "version": "1.1.4",
  "description": "Serbian (Cyrillic) dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "conditional-build": "pnpm run build --conditional",
    "prepublishOnly": "echo pre-publish",
    "test": "codetypo -c ./codetypo-ext.json --locale=sr-Cyrl \"samples/**\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/sr_Cyrl"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "sr_Cyrl",
    "Serbian (Cyrillic)",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/sr_Cyrl#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "dict/sr-cyrl.trie",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
