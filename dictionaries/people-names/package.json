{
  "name": "@codetypo/dict-people-names",
  "version": "1.1.8",
  "description": "People Names dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "head -n 1000 \"src/names.txt\" | codetypo -c ./codetypo-ext.json  \"--languageId=*\" stdin",
    "prepublishOnly": "echo OK",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/people-names"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "people-names",
    "People Names",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/people-names#readme",
  "devDependencies": {
    "@internal/en-freq": "workspace:*"
  },
  "dependencies": {},
  "files": [
    "dict/people-names.txt",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
