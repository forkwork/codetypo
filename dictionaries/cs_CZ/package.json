{
  "name": "@codetypo/dict-cs-cz",
  "version": "3.0.6",
  "description": "Czech dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build && pnpm prepare:dictionary",
    "build:conditional": "codetypo-tools-cli build --conditional && pnpm prepare:dictionary",
    "test": "pipe-cmds \"hunspell-reader words -n 1000 src/Czech.dic\" \"codetypo -c ./codetypo-ext.json --locale=cs --languageId=* stdin\"",
    "prepare:dictionary": "codetypo-tools-cli gzip \"*.trie\"",
    "prepublishOnly": "echo pre-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/cs_CZ"
  },
  "keywords": [
    "codetypo",
    "cs_CZ",
    "Czech",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft.nl>",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/cs_CZ#readme",
  "files": [
    "Czech.txt",
    "Czech.trie.gz",
    "codetypo-ext.json"
  ]
}
