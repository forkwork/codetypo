{
  "name": "@codetypo/dict-la",
  "version": "1.0.4",
  "description": "Latin dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build && pnpm gz",
    "build:conditional": "codetypo-tools-cli build --conditional && pnpm gz",
    "test:dic": "pipe-cmds \"hunspell-reader words -n 1000 \\\"src/hunspell/la.dic\\\"\" \"codetypo -c ./codetypo-ext.json \\\"--locale=la,la-VA\\\" \\\"--languageId=*\\\" stdin\"",
    "test:samples": "codetypo samples",
    "test": "pnpm test:dic && pnpm test:samples",
    "gz": "codetypo-tools-cli gzip \"*.trie\"",
    "prepare:dictionary": "pnpm gz",
    "prepublishOnly": "echo pre-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/la"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "la",
    "Latin",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "contributors": [],
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/la#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "la.trie.gz",
    "src/hunspell/la_license.txt",
    "codetypo-ext.json"
  ]
}
