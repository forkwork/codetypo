{
  "name": "@codetypo/dict-scientific-terms-gb",
  "version": "1.0.4",
  "description": "Scientific Terms GB dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build && pnpm gz",
    "conditional-build": "codetypo-tools-cli build --conditional && pnpm gz",
    "test": "codetypo samples",
    "gz": "codetypo-tools-cli gzip \"dict/*.trie\"",
    "prepare:dictionary": "pnpm gz",
    "prepublishOnly": "echo pre-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/scientific_terms_GB"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "scientific_terms_GB",
    "Scientific Terms GB",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/scientific_terms_GB#readme",
  "files": [
    "dict/scientific-terms-gb.trie.gz",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
