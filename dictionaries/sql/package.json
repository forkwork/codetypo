{
  "name": "@codetypo/dict-sql",
  "version": "2.2.0",
  "description": "SQL dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "pnpm test-dict && pnpm test-samples",
    "test-dict": "shx head -n 1000 \"src/sql.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=sql\" stdin",
    "test-samples": "codetypo \"samples/**\"",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/sql"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "sql",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/sql#readme",
  "files": [
    "sql.txt.gz",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
