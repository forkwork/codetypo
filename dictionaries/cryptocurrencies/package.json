{
  "name": "@codetypo/dict-cryptocurrencies",
  "version": "5.0.4",
  "description": "Cryptocurrencies dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
