    "codetypo",
    "Cryptocurrencies",
    "Cryptocurrencies",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo-dicts/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo-dicts/blob/main/dictionaries/cryptocurrencies#readme",
  "files": [
    "dict/*.txt",
    "codetypo-ext.json"
  ]
}
