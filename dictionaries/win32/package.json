{
  "name": "@codetypo/dict-win32",
  "version": "2.0.7",
  "description": "Win32 dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "pnpm run test-src && pnpm run test-samples",
    "test-src": "shx head -n 10000 \"src/win32.txt\" | codetypo -c ./codetypo.json \"--locale=*\" stdin",
    "test-samples": "codetypo samples/**",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/win32"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "Win32",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/Win32#readme",
  "files": [
    "win32.txt.gz",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
