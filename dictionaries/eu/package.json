{
  "name": "@codetypo/dict-eu",
  "version": "1.0.4",
  "description": "Basque dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "conditional-build": "pnpm run sync && pnpm run build --conditional",
    "sync": "echo OK || pnpm cpy \"node_modules/basque_hunspell/eu.*\" \"src/basque_hunspell\" --flat",
    "test": "pipe-cmds \"hunspell-reader words -n 1000 -m 1 \\\"src/basque_hunspell/eu.dic\\\"\" \"codetypo -c ./codetypo-ext.json \\\"--locale=eu\\\" \\\"--languageId=*\\\" stdin\"",
    "prepublishOnly": "echo OK",
    "prepare:dictionary": "echo OK"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/eu"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "eu",
    "Basque",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/eu#readme",
  "files": [
    "dict/eu.trie",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
