{
  "name": "@codetypo/dict-kotlin",
  "version": "1.1.0",
  "description": "Kotlin dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "pnpm test:samples && pnpm test:src",
    "test:samples": "codetypo \"samples/*.kt\"",
    "test:src": "head -n 1000 \"src/kotlin.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=kotlin\" stdin",
    "prepublishOnly": "echo OK",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/kotlin"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "kotlin",
    "Kotlin",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/kotlin#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "dict/kotlin.txt",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
