{
  "name": "@codetypo/dict-al",
  "version": "1.1.0",
  "description": "AL Dictionary",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "shx head -n 1000 \"src/al.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=AL\" stdin",
    "prepublishOnly": "echo OK",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/al"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "dict-al",
    "AL",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft",  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/dict-al#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "dict/al.txt",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
