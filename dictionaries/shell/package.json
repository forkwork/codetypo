{
  "name": "@codetypo/dict-shell",
  "version": "1.1.0",
  "description": "Shell Script dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "codetypo-tools-cli build",
    "test": "shx cat test/legacy-words.txt | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=shellscript\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasofto-dicts",
    "directory": "dictionaries/shell"
  },
  "keywords": [
    "codetypo",
    "bash",
    "shell",
    "POSIX",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasofto-dicts/issues"
  },
  "homepage": "https://github.com/khulnasofto-dicts/blob/main/dictionaries/bash#readme",
  "files": [
    "dict/*",
    "codetypo-ext.json"
  ]
}
