{
  "name": "@codetypo/dict-monkeyc",
  "version": "1.0.10",
  "description": "Monkey C dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 codetypo-tools-cli compile \"src/monkeyc_keywords.txt\" -o .",
    "test": "shx head -n 1000 \"src/monkeyc_keywords.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=monkeyc\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/monkeyc"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "monkeyc",
    "Monkey C",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/monkeyc#readme",
  "files": [
    "monkeyc_keywords.txt.gz",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
