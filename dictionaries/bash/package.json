{
  "name": "@codetypo/dict-bash",
  "version": "4.2.0",
  "description": "Bash dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "echo ok",
    "test": "shx cat samples/bash-words.txt | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=shellscript\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/bash"
  },
  "keywords": [
    "codetypo",
    "bash",
    "bash",
    "dictionary",
    "spelling"
  ],
  "author": "KhulnaSoft Ltd <support@khulnasoft.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/bash#readme",
  "files": [
    "dict/*",
    "codetypo-ext.json"
  ],
  "dependencies": {
    "@codetypo/dict-shell": "workspace:*"
  }
}
