{
  "name": "@codetypo/dict-r",
  "version": "2.1.0",
  "description": "R dictionary for codetypo.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./codetypo-ext.json",
    "./codetypo": "./codetypo-ext.json",
    "./codetypo-ext.json": "./codetypo-ext.json"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 codetypo-tools-cli compile --split \"src/r.txt\" -o .",
    "test": "shx head -n 1000 \"src/r.txt\" | codetypo -c ./codetypo-ext.json \"--locale=*\" \"--languageId=r\" stdin",
    "prepublishOnly": "echo pre-publish",
    "prepare:dictionary": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo",
    "directory": "dictionaries/r"
  },
  "keywords": [
    "codetypo",
    "codetypo-ext",
    "r",
    "R",
    "dictionary",
    "spelling"
  ],
  "author": "Street Side Software <support@khulnasoft.nl>",
  "contributors": [
    "Matthew Toohey <contact@mtoohey.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/issues"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/blob/main/dictionaries/r#readme",
  "files": [
    "r.txt.gz",
    "codetypo-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
