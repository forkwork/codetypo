{
  "name": "@codetypo/codetypo-bundled-dicts",
  "version": "8.17.3",
  "description": "Dictionaries bundled with codetypo",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "type": "module",
  "sideEffects": false,
  "main": "codetypo-default.json",
  "files": [
    "codetypo-default.json",
    "codetypo-default.config.js",
    "codetypo-compatibility.json",
    "compatibility-words.txt.gz"
  ],
  "exports": {
    ".": "./codetypo-default.config.js",
    "./codetypo-default.config.js": "./codetypo-default.config.js",
    "./codetypo-default.json": "./codetypo-default.json"
  },
  "scripts": {
    "build": "tsc -p .",
    "build-dict": "codetypo-tools-cli compile compatibility-words.txt",
    "clean": "shx rm -rf dist temp coverage \"*.tsbuildInfo\"",
    "clean-build": "pnpm run clean && pnpm run build",
    "watch": "pnpm run build --watch",
    "test": "node ../../bin.mjs \"**/*.{txt,md,ts}\"",
    "update-fix-en-gb": "pnpm add @codetypo/dict-en-gb@1.1.33",
    "update-dictionaries": "pnpm update --latest \"@codetypo/dict-*\" \"!@codetypo/dict-en-gb\" --depth 1 --filter-prod . && pnpm update-fix-en-gb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo.git",
    "directory": "packages/codetypo-bundled-dicts"
  },
  "keywords": [
    "spell",
    "checker",
    "codetypo",
    "dictionaries"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/labels/codetypo-bundled-dicts"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/tree/main/packages/codetypo-bundled-dicts#readme",
  "dependencies": {
    "@codetypo/dict-ada": "../dictionaries/*",
    "@codetypo/dict-al": "../dictionaries/*",
