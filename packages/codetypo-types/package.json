{
  "name": "@codetypo/codetypo-types",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "version": "8.17.3",
  "description": "Types for codetypo and codetypo-lib",
  "type": "commonjs",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./Parser": {
      "import": "./dist/Parser/index.mjs",
      "require": "./dist/Parser/index.js"
    },
    "./codetypo.schema.json": {
      "import": "./codetypo.schema.json",
      "require": "./codetypo.schema.json"
    },
    "./ajv.config.js": {
      "require": "./ajv.config.js"
    }
  },
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*.d.ts": [
        "dist/*.d.ts"
      ],
      "*.d.mts": [
        "dist/*.d.mts"
      ],
      "index.d.ts": [
        "dist/index.d.ts"
      ],
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "Parser": [
        "dist/Parser/index.d.ts"
      ],
      "*": [
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "shx rm -rf dist temp coverage \"*.tsbuildInfo\"",
    "build": "pnpm build:tsc && pnpm build:tsup && pnpm run build-schema",
    "build:tsc": "tsc -p tsconfig.json",
    "build:tsup": "tsup-node --entry src/index.ts --entry src/Parser/index.ts --format cjs,esm  --dts --out-dir dist",
    "build-schema": "pnpm build-codetypo-schema",
    "clean-build": "pnpm run clean && pnpm run build",
    "coverage": "vitest run --coverage",
    "test": "vitest run",
    "prepare": "echo Skip Building Types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo.git",
    "directory": "packages/codetypo-types"
  },
  "files": [
    "dist",
    "ajv.config.js",
    "codetypo.schema.json",
    "!**/*.tsbuildInfo",
    "!**/__mocks__",
    "!**/*.test.*",
    "!**/test/**",
    "!**/test.*.*",
    "!**/*.spec.*",
    "!**/*.map"
  ],
  "keywords": [
    "codetypo",
    "types"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/tree/main/packages/codetypo-types#readme"
}
