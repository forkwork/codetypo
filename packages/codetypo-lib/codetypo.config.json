{
    "$schema": "https://raw.githubusercontent.com/khulnasoft/codetypo/main/codetypo.schema.json",
    "version": "0.2",
    "id": "codetypo-package-config",
    "name": "codetypo Package Config",
    "language": "en",
    "words": ["gensequence"],
    "maxNumberOfProblems": 100,
    "ignorePaths": [
        "node_modules/**",
        "coverage/**",
        "dist/**",
        "package.json",
        "**/package.json",
        "**/.codetypo.json",
        ".vscode/**"
    ],
    "allowCompoundWords": false,
    "dictionaryDefinitions": [],
    "ignoreWords": ["CodeTypoSettingsWSTO"],
    "import": ["../../codetypo.json"]
}
