{
  "name": "codetypo-lib",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "version": "8.17.2",
  "description": "A library of useful functions used across various codetypo tools.",
  "type": "module",
  "sideEffects": false,
  "types": "dist/lib/index.d.ts",
  "module": "dist/lib/index.js",
  "exports": {
    ".": {
      "import": "./dist/lib/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildInfo",
    "!**/__mocks__",
    "!**/test*/**",
    "!**/test.*",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!**/*.map"
  ],
  "scripts": {
    "clean": "shx rm -rf dist temp coverage \"*.tsbuildInfo\"",
    "clean-build": "pnpm clean && pnpm build",
    "build": "tsc -p . && pnpm run build:api",
    "build:api": "rollup -c api/rollup.config.mjs",
    "watch": "tsc -p . --watch",
    "coverage": "vitest run --coverage --pool=forks",
    "test-watch": "vitest",
    "prepublishOnly": "pnpm run clean-build",
    "#test": "vitest run --reporter=hanging-process --reporter=default",
    "test": "vitest run --pool=forks",
    "test:update-snapshot": "vitest run -u",
    "test:perf": "NODE_ENV=production insight --file \"**/*.perf.{mjs,js}\"",
    "test:perf:ts": "NODE_ENV=production insight --file \"**/*.perf.{mts,ts}\" --register ts-node/esm",
    "test:perf:prof": "NODE_ENV=production node --cpu-prof ../../node_modules/perf-insight/bin.mjs --file \"dist/**/*.perf.{mjs,js}\" -t 1000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/codetypo.git",
    "directory": "packages/codetypo-lib"
  },
  "keywords": [
    "spell",
    "checker",
    "code",
    "camel",
    "case",
    "spelling",
    "spell checker",
    "spelling checker",
    "lint"
  ],
  "author": "Md Sulaiman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khulnasoft/codetypo/labels/codetypo-lib"
  },
  "homepage": "https://github.com/khulnasoft/codetypo/tree/main/packages/codetypo-lib#readme",
  "dependencies": {
    "@codetypo/codetypo-bundled-dicts": "workspace:*",
    "@codetypo/codetypo-pipe": "workspace:*",
    "@codetypo/codetypo-resolver": "workspace:*",
    "@codetypo/codetypo-types": "workspace:*",
    "@codetypo/dynamic-import": "workspace:*",
    "@codetypo/filetypes": "workspace:*",
    "@codetypo/strong-weak-map": "workspace:*",
    "@codetypo/url": "workspace:*",
    "clear-module": "^4.1.2",
    "comment-json": "^4.2.5",
    "codetypo-config-lib": "workspace:*",
    "codetypo-dictionary": "workspace:*",
    "codetypo-glob": "workspace:*",
    "codetypo-grammar": "workspace:*",
    "codetypo-io": "workspace:*",
    "codetypo-trie-lib": "workspace:*",
    "env-paths": "^3.0.0",
    "fast-equals": "^5.2.2",
    "gensequence": "^7.0.0",
    "import-fresh": "^3.3.0",
    "resolve-from": "^5.0.0",
    "vscode-languageserver-textdocument": "^1.0.12",
    "vscode-uri": "^3.0.8",
    "xdg-basedir": "^5.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@codetypo/dict-cpp": "workspace:*",
    "@codetypo/dict-csharp": "workspace:*",
    "@codetypo/dict-css": "workspace:*",
    "@codetypo/dict-fa-ir": "workspace:*",
    "@codetypo/dict-fr-fr": "workspace:*",
    "@codetypo/dict-html": "workspace:*",
    "@codetypo/dict-nl-nl": "workspace:*",
    "@codetypo/dict-python": "workspace:*",
    "@types/configstore": "^6.0.2",
    "configstore": "^7.0.0",
    "leaked-handles": "^5.2.0",
    "lorem-ipsum": "^2.0.8",
    "perf-insight": "^1.2.0"
  }
}
