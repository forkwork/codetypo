{
    "version": "0.2",
    "files": [
        "**/*.{md,txt}"
    ],
    "import": [
        "./codetypo-ext.json"
    ],
    "ignorePaths": ["codetypo-ext.json", "*.trie", "/src/**"],
    "ignoreWords": [
        "frami",
        "Huggenberger",
        "igerman",
        "Naber",
        "zeiler",
        "Zeiler"
    ],
    "overrides": [
        {
            "filename": "samples/**/*.md",
            "caseSensitive": true,
            "dictionaries": [
                "de-de"
            ]
        }
    ]
}
